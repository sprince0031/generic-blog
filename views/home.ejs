<%- include('head') -%>
<!-- Add page specific head content here -->
<%- include('navbar') -%>

  
    <h1 class="heading">Sprince0031's Personal Blog</h1>
    <p class="home-content"><%= homeStartingContent %></p>

    <% posts.forEach(post => { %>
        <h2 class="post-title"><%= post.title %></h2>
        <p class="post-desc">
            <%= post.content[0].substring(0, 200) %>...
            <a href="/posts/<%= post.title %>">Read more</a>
        </p>
    <% }); %>

  

<%- include('footer') -%>